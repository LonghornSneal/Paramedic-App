    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome (WSL)",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://localhost:5173",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Launch Chrome (loopback)",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://127.0.0.1:5173",
            "webRoot": "${workspaceFolder}"
        }
    ]
}
